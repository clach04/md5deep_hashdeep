cross: ${WINDOC}
	./configure --host=mingw32
	make all
	mingw32-strip ${ALL_GOALS}deep.exe
	zip -9 md5deep-${VERSION}.zip ${ALL_GOALS}deep.exe $(WINDOC)
	rm -f ${WINDOC}
	make distclean

world: dist cross
