reconfig:
	aclocal && autoconf && autoheader && automake && ./config.status

CHANGES.TXT: NEWS
	cp NEWS CHANGES.TXT
	flip -d CHANGES.TXT
#	unix2dos CHANGES.TXT

README.TXT:
	/usr/bin/tbl ./$(man_MANS) | /usr/bin/groff -S -Wall -mtty-char -mandoc -Tascii | /usr/bin/col -bx > README.TXT
	flip -d README.TXT
